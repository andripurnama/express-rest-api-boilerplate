{
  "name": "express-rest-api-postgres",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node -r module-alias/register build/app.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node -r module-alias/register server/index.js --ignore *.test.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf build && babel src --out-dir build",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:undo:all": "sequelize db:migrate:undo:all",
    "migration:create": "sequelize migration:create --name",
    "seed": "sequelize db:seed:all",
    "seed:create": "sequelize seed:create --name",
    "seed:undo": "sequelize db:seed:undo",
    "seed:undo:all": "sequelize db:seed:undo:all"
  },
  "_moduleAliases": {
    "@": "src"
  },
  "keywords": [
    "rest api",
    "api",
    "postgres",
    "tyeporm",
    "node",
    "node.js",
    "boilerplate",
    "express",
    "sequelize",
    "express-validator",
    "eslint",
    "prettier"
  ],
  "author": "Andri Purnama",
  "license": "MIT",
  "description": "An NodeJs Express boilerplate for REST API project",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "http-status": "^1.7.4",
    "module-alias": "^2.2.3",
    "nodemailer": "^6.9.13",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/node": "^7.24.7",
    "@babel/plugin-transform-class-properties": "^7.24.7",
    "@babel/plugin-transform-object-rest-spread": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.4.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.1",
    "prettier": "^3.3.1",
    "rimraf": "^5.0.7",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0"
  }
}
